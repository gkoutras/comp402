/* This is a program that computes and prints the first 10 Fibonacci numbers*/

const message = "Type the number of the Fibonacci numbers: " string;

func begin() {	
    var i, f0 = 0, f1 = 1, f, n int;

    writeString(message);
    n = readInt();

    for(i = 1; 1 <= n; i = i + 1) {
        f = f0 + f1;
        writeInt(f);
        f1 = f0;
        f0 = f1;
    }

    return 0;
}
